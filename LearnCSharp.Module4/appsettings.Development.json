{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/Module3Log-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 3
        }
      },
      {
        "Name": "Console"
      }
    ]
  }
}